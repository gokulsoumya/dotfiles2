clear

alt+2 start
Escape end

w cut-up
a cut-left
s cut-down
d cut-right

shift+a move-left
shift+s move-down
shift+w move-up
shift+d move-right

q cut-left,cut-up
e cut-right,cut-up
z cut-left,cut-down
c cut-right,cut-down

x history-back

f warp,click 1,end
Shift_L+f warp,click 3,end
space warp,end
g click 1,end
Shift_L+g click 3,end

v sh "xdotool search --name ' - Vivaldi' windowactivate",end
t sh "xdotool search --name ' - byobu' windowactivate",end
2 sh "xdotool search --name ' - byobu' windowactivate && tmux select-window -t dashboard && tmux select-pane -t:.1 && tmux send-keys R",end
1 sh "xdotool search --name ' - byobu' windowactivate && tmux select-window -t dashboard && tmux select-pane -t:.0",end 
h sh "xdotool search --name ' - byobu' windowactivate && tmux select-window -t dashboard && tmux select-pane -t:.3",end 

3 cut-up 0.01,warp,end
4 cut-up 0.05,cut-right 0.20,warp,click 1,end
